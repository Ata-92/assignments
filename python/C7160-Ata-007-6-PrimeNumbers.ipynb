    "n = int(input(\"Please enter a prime number limit : \"))\n",
    "nums = []\n",
    "for i in range(2, n):\n",
    "    for ii in range(2, 8):\n",
    "        if i in [2, 3, 5, 7] or i % ii != 0:\n",
    "            if ii == 7:\n",
    "                nums.append(i)\n",
    "        else:\n",
    "            break        \n",
    "print(nums)"
   ]
